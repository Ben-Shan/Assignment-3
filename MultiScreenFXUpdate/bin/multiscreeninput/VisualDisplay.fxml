<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="multiscreeninput.VisualDisplayController">
   <children>
      <BorderPane nodeOrientation="RIGHT_TO_LEFT" prefHeight="346.0" prefWidth="336.0">
         <bottom>
            <Button fx:id="closeBtn" mnemonicParsing="false" onAction="#closeWindow" text="Close" textAlignment="RIGHT" BorderPane.alignment="CENTER_LEFT" />
         </bottom>
         <center>
            <Canvas fx:id="canvas" height="196.0" width="295.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <Pane fx:id="topPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
